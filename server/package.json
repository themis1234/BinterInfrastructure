{
  "name": "qr-code-tracker",
  "version": "1.0.0",
  "description": "QR Code tracking system with Express.js, TypeScript, and PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "build-local": "tsc",
    "build": "npm install && tsc && npm run migrate",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/index.js",
    "migrate": "ts-node src/database/migrations/runMigrations.ts"
  },
  "keywords": ["qr-code", "tracking", "express", "typescript", "postgresql"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.10.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/cors": "^2.8.13",
    "@types/uuid": "^9.0.2",
    "typescript": "^5.1.6",
    "ts-node-dev": "^2.0.0"
  }
}